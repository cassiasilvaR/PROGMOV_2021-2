<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ESHELF" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_slate_gray</item>
        <item name="colorPrimaryDark">@color/dark_jungle_green</item>
        <item name="colorAccent">@color/russian_green</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ESHELF.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ESHELF.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ESHELF.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>